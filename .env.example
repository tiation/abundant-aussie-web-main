# Abundant Aussie Web - Environment Configuration
# ChaseWhiteRabbit NGO Project

# Application Configuration
NODE_ENV=development
VITE_APP_NAME="Abundant Aussie Web"
VITE_APP_VERSION="1.0.0"
VITE_API_BASE_URL="http://localhost:3000/api"

# ChaseWhiteRabbit NGO Configuration
VITE_NGO_NAME="ChaseWhiteRabbit"
VITE_NGO_WEBSITE="https://chasewhiterabbit.org"
VITE_NGO_EMAIL="support@chasewhiterabbit.org"

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# Analytics Configuration (Optional)
VITE_ANALYTICS_ID=""
VITE_ANALYTICS_PROVIDER=""

# API Configuration
VITE_API_TIMEOUT=10000
VITE_API_RETRY_ATTEMPTS=3

# Social Media Links
VITE_SOCIAL_GITHUB="https://github.com/ChaseWhiteRabbit"
VITE_SOCIAL_TWITTER=""
VITE_SOCIAL_LINKEDIN=""

# Deployment Configuration
DOCKER_REGISTRY="docker.sxc.codes"
KUBERNETES_NAMESPACE="abundant-aussie"
HELM_CHART_VERSION="1.0.0"

# Infrastructure URLs
GRAFANA_URL="https://grafana.sxc.codes"
ELASTICSEARCH_URL="https://elastic.sxc.codes"
SUPABASE_URL="https://supabase.sxc.codes"

# Security Configuration
VITE_CSP_POLICY="default-src 'self'"
VITE_HSTS_MAX_AGE="31536000"

# Development Tools
VITE_DEVTOOLS_ENABLED=true
VITE_HOT_RELOAD=true
