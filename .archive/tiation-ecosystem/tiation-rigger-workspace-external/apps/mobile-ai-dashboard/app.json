{
  "expo": {
    "name": "RiggerHub AI Dashboard",
    "slug": "riggerhub-ai-dashboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.riggerhub.ai.dashboard"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A0A0A"
      },
      "package": "com.riggerhub.ai.dashboard"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "riggerhub-ai"
  }
}