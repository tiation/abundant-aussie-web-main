{
  "extends": [
    "next/core-web-vitals",
    "eslint-config-prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "@next/next/no-img-element": "error",
    "@next/next/no-html-link-for-pages": "error",
    "react-hooks/exhaustive-deps": "warn",
    "react/jsx-no-target-blank": "error",
    "react/jsx-key": "error",
    "no-unused-vars": "warn",
    "no-console": "warn",
    "prefer-const": "error",
    "no-var": "error"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
